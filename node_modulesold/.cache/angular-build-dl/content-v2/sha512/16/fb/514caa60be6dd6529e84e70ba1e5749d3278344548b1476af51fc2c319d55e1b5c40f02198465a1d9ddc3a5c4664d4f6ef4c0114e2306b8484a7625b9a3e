    loadData(infiniteScroll) {
        this.offset = parseInt(this.offset) + 6;
        this.api.getJobList(this.offset)
            .pipe((0,rxjs_operators__WEBPACK_IMPORTED_MODULE_4__.finalize)(() => { }))
