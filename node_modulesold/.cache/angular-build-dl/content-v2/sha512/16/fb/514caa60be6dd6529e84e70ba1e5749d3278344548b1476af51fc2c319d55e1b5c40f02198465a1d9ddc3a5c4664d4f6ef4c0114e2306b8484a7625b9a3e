        });
    }
    viewJobLocationPermission() {
        this.isAPILoaded = false;
        // search first time with default location of loggedin user
        this.storage.get(_api_petcloud_api_service__WEBPACK_IMPORTED_MODULE_2__.PetcloudApiService.USER).then((userData) => (0,tslib__WEBPACK_IMPORTED_MODULE_12__.__awaiter)(this, void 0, void 0, function* () {
