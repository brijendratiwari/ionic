                    if (authStatus == "authorized" || authStatus == "authorized_when_in_use"
                        || authStatus == "GRANTED" || authStatus == "GRANTED_WHEN_IN_USE") {
                        yield this.geolocation.getCurrentPosition().then((resp) => (0,tslib__WEBPACK_IMPORTED_MODULE_12__.__awaiter)(this, void 0, void 0, function* () {
                            if (resp) {
                                this.jobpostingSearch.lat = yield resp.coords.latitude.toString();
                                this.jobpostingSearch.lng = yield resp.coords.longitude.toString();
                                this.jobpostingSearchData.lat = yield resp.coords.latitude.toString();
                                this.jobpostingSearchData.lng = yield resp.coords.longitude.toString();
