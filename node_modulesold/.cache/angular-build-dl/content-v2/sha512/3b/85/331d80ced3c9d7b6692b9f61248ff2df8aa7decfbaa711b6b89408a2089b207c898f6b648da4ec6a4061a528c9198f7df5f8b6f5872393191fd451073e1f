          key: "setDefaultMethod",
          value: function setDefaultMethod(ev) {
            if (this.stripeDefaultCheck === true) {
              this.stripeDefaultid = 1;
            } else {
              this.stripeDefaultid = '';
            }
          }
          /**
           * Save card in stripe using api
           */

        }, {
          key: "saveCardInStripe",
          value: function saveCardInStripe() {
            var _this4 = this;

            var card = {
              number: this.creditCardFrm.value.cardNumber,
              name: this.creditCardFrm.value.cardName,
              expMonth: this.creditCardFrm.value.cardMonth,
              expYear: this.creditCardFrm.value.cardYear,
              cvc: this.creditCardFrm.value.cardCcv
            };
            this.api.showLoader();
            this.stripe.createCardToken(card).then(function (token) {
              if (token) {
                // send details in stripe
                var cardFrm = {
                  'post_type': 'add_card',
                  'redirect': '',
                  'stripeDefault': '',
                  'stripeToken': token.id
                };
                console.log('cardFrm..............', cardFrm);

                _this4.creditCardFrm.setValue({
                  cardName: token.card.name,
                  cardNumber: 'xxxxxxxxxxxx' + token.card.last4,
                  cardMonth: token.card.exp_month,
                  cardYear: token.card.exp_year,
                  cardCcv: ''
                });

                _this4.api.updateCardInStripe(cardFrm).pipe((0, rxjs_operators__WEBPACK_IMPORTED_MODULE_6__.finalize)(function () {
                  _this4.api.hideLoader();
                })).subscribe(function (res) {
                  if (res.success) {
                    _this4.storage.set(_api_petcloud_api_service__WEBPACK_IMPORTED_MODULE_2__.PetcloudApiService.STRIPECARD, token.card);

                    _this4.api.showToast('card details updated in stripe', 2000, 'bottom');

                    _this4.navCntl.navigateRoot('/home/tabs/profile-menu');
                  } else {
                    _this4.api.showToast(res.error, 2000, 'bottom');
                  }
                }, function (err) {
                  _this4.api.autoLogout(err, cardFrm);
                });
              } else {
                _this4.navCntl.navigateRoot('/home/tabs/profile-menu');

                _this4.api.showToast('card details not updated in stripe, Please try again!', 3000, 'bottom');
              }
            })["catch"](function (error) {
              _this4.api.hideLoader();

              _this4.api.showToast(error, 3000, 'bottom');
            });
          }
          /**
           * convert number to array
           * @param num pass number
           */

        }, {
          key: "getCardYears",
          value: function getCardYears() {
            var today = new Date();
            var startYear = today.getFullYear();
            var endYear = today.getFullYear() + 30;
            var yearArr = [];

            for (var i = startYear; i <= endYear; i++) {
              yearArr.push(i);
            }

            return yearArr;
          }
        }, {
          key: "connectPaypal",
          value: function connectPaypal() {
            this.api.showToast("We will release paypal payments in next version of App", "3000", "bottom");
          }
        }, {
          key: "getInfo",
          value: function getInfo() {
            var _this5 = this;

            this.storage.get(_api_petcloud_api_service__WEBPACK_IMPORTED_MODULE_2__.PetcloudApiService.USER).then(function (user) {
              console.log(user.stripe_card);

