        }, {
          key: "idfaTracking",
          value: function idfaTracking() {
            var idfaPlugin = cordova.plugins.idfa;
            idfaPlugin.getInfo().then(function (info) {
              console.log("idfaPlugin.getInfo 1", info);

              if (!info.trackingLimited) {
                return info.idfa || info.aaid;
              } else if (info.trackingPermission === idfaPlugin.TRACKING_PERMISSION_NOT_DETERMINED) {
                return idfaPlugin.requestPermission().then(function (result) {
                  if (result === idfaPlugin.TRACKING_PERMISSION_AUTHORIZED) {
                    return idfaPlugin.getInfo().then(function (info) {
                      console.log("idfaPlugin.getInfo", info);
                      return info.idfa || info.aaid;
                    });
                  }
                });
              }
            }).then(function (idfaOrAaid) {
              if (idfaOrAaid) {
                console.log("idfaOrAaid", idfaOrAaid);
              }
            });
          }
        }]);

        return AppComponent;
      }();

