              this.selectedFilter.end_date = "";
              this.isFirstLoad = false;
            }

            this.minToDate = event.value;
          }
        }, {
          key: "getAllService",
          value: function getAllService() {
            var _this131 = this;

            this.api.showLoader();
            this.api.getAllService().pipe((0, rxjs_operators__WEBPACK_IMPORTED_MODULE_3__.finalize)(function () {
              _this131.api.hideLoader();
            })).subscribe(function (res) {
              _this131.isAPILoaded = true;
              _this131.primaryService = res.primary;
              var iconCount = 1;

              for (var i = 0; i < _this131.primaryService.length; i++) {
                _this131.primaryService[i]['serviceIcon'] = 'filter_b_' + iconCount + '.svg';
                iconCount++;
              }
            }, function (err) {
              _this131.isAPILoaded = false;

              _this131.api.autoLogout(err, "");
            });
          }
        }, {
          key: "setFilter",
          value: function setFilter(key, val) {
            var mainVal = val;

            if (key == 'serviceTypeId' || key == 'petWeight' || key == 'petTypes' || key == 'bookingtype') {
              var existValue = this.selectedFilter[key] ? this.selectedFilter[key].split(',') : '';

              if (existValue && existValue.length > 0) {
                var index = existValue.findIndex(function (d) {
                  return parseInt(d) == val;
                });

                if (index > -1) {
                  mainVal = existValue.splice(index, 1);
                } else {
                  existValue.push(val.toString());
                }

                mainVal = existValue.join();
              } else {
                mainVal = val.toString();
              }
            }

            this.selectedFilter[key] = mainVal;
          }
        }, {
          key: "isSelected",
          value: function isSelected(key, val) {
            var existValue = this.selectedFilter[key] ? this.selectedFilter[key].split(',') : '';

            if (existValue && existValue.length > 0) {
              var index = existValue.findIndex(function (d) {
                return parseInt(d) == val;
              });
              return index > -1;
            }

            return false;
          }
        }, {
          key: "applyFilter",
          value: function applyFilter() {
            this.modal.dismiss(this.selectedFilter);
          }
        }, {
          key: "clearFilter",
          value: function clearFilter() {
            this.selectedFilter = {
              breed: '',
              distance: 0,
              start_date: '',
              end_date: '',
              petWeight: "",
              serviceTypeId: '',
              petTypes: '',
              ratepernight: 0,
              bookingtype: ''
            };
            this.modal.dismiss(this.selectedFilter);
          }
        }, {
          key: "changeBookingtype",
          value: function changeBookingtype() {
            var existValue = this.selectedFilter.bookingtype ? this.selectedFilter.bookingtype.split(',') : '';

            if (existValue && existValue.length > 0) {
              this.allFtrequency = false;
              var index = existValue.findIndex(function (d) {
                return parseInt(d) == 0;
              });
              var index2 = existValue.findIndex(function (d) {
                return parseInt(d) == 1;
              });
              this.onceOffJob = index > -1;
              this.recurringJob = index2 > -1;
            } else {
              this.allFtrequency = true;
              this.onceOffJob = false;
              this.recurringJob = false;
            }
          }
        }, {
          key: "changeBooking",
          value: function changeBooking(val) {
            var _this132 = this;

            setTimeout(function () {
              if (val === '') {
                _this132.selectedFilter.bookingtype = '';
              } else {
                _this132.setFilter('bookingtype', val);
              }

              _this132.changeBookingtype();
            }, 200);
          }
