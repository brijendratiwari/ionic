            var to = this.userId == this.minderId ? this.ownerId : this.minderId;

            if (this.chatMessage != "" || this.petReport != undefined) {
              if (this.petReport != "" || this.chatMessage.replace(/^\s+/, "").replace(/\s+$/, "")) {
