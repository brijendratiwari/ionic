          key: "onOtherAmount",
          value: function onOtherAmount(event) {
            this.tempAmount = event;
            this.creditAmount = event;

            if (event != "") {
              this.calculateTransactionalAmount(this.tempAmount);
            } else {
              this.transactionalFees = 0;
              this.payableAmount = 0;
              this.tempAmount = 0;
              this.creditAmount = 0;
