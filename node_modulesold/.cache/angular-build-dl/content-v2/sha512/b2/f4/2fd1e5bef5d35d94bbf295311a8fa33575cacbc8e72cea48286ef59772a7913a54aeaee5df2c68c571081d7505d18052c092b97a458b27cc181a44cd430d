                  message: this.chatMessage,
                  type: "message",
                  to: to,
                  picture: null,
                  dateTime: this.api.getDateTime(),
                  isMessageScene: this.isOnline == "Online" ? true : false,
                  petReport: this.petReport == undefined ? "" : this.petReport
                };
                this.pushToken(this.chatMessage, ""); // this.scrollToBottom();

                this.chatsService.addChats(chat, this.bookingId).then(function (res) {
