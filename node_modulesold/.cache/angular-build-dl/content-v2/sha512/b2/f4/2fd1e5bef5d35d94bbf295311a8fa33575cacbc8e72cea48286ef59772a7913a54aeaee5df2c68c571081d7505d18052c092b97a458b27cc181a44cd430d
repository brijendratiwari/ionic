          key: "validateAndRedirect",
          value: function validateAndRedirect(herf) {
            if (herf) {
              var isValid = this.api.validURL(herf);

              if (isValid && !this.isOpening && this.platform.is('cordova')) {
                this.isOpening = true;
                this.api.openExteralLinks(herf);
              }
            }

            this.isOpening = false;
          }
        }, {
          key: "isMessageonChange",
