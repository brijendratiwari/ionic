let PaymentMethodPage = class PaymentMethodPage {
    constructor(storage, api, stripe, navCntl, router, plt, route, formBuilder) {
        // this.backButtonEvent();
        this.storage = storage;
        this.api = api;
        this.stripe = stripe;
        this.navCntl = navCntl;
        this.router = router;
        this.plt = plt;
        this.route = route;
        this.formBuilder = formBuilder;
        this.paypal_email = '';
        this.stripeDefaultCheck = false;
        this.backButton = "";
    }
    ngOnInit() {
        this.getInfo();
        this.storage.get(_api_petcloud_api_service__WEBPACK_IMPORTED_MODULE_2__.PetcloudApiService.USER)
            .then((userData) => {
            this.paypal_email = userData.paypal_email;
        });
        this.creditCardFrm = this.formBuilder.group({
            cardName: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_4__.Validators.required]],
            cardNumber: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_4__.Validators.required, _angular_forms__WEBPACK_IMPORTED_MODULE_4__.Validators.maxLength(16), _angular_forms__WEBPACK_IMPORTED_MODULE_4__.Validators.pattern('[0-9]+$')]],
            cardMonth: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_4__.Validators.required]],
            cardYear: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_4__.Validators.required]],
            cardCcv: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_4__.Validators.required, _angular_forms__WEBPACK_IMPORTED_MODULE_4__.Validators.maxLength(4), _angular_forms__WEBPACK_IMPORTED_MODULE_4__.Validators.pattern('[0-9]+$')]]
        });
    }
    backButtonNavigate() {
        if (this.backButton) {
            this.router.navigateByUrl('/home/tabs/sitter-listing');
        }
        else {
            this.navCntl.pop();
        }
    }
    ionViewDidEnter() {
        this.backButton = this.route.snapshot.paramMap.get("backBtn");
        this.storage.get(_api_petcloud_api_service__WEBPACK_IMPORTED_MODULE_2__.PetcloudApiService.USER)
            .then((res) => (0,tslib__WEBPACK_IMPORTED_MODULE_5__.__awaiter)(this, void 0, void 0, function* () {
            let stripeData = res.stripe;
            let public_key = yield stripeData.public_key;
            this.stripe.setPublishableKey(public_key);
            this.getCard();
            if (res.user_type == 2 || res.user_type == 3) {
                if (res.verified == 0 || res.verify_phoneflag == "N" || !res.isBackgroundChecked ||
                    !res.isRightToWorkChecked || !res.isAnimalCareChecked) {
                    yield this.api.isVerificationPendingModel();
                }
            }
            else if (res.user_type == 1) {
                if (res.verified == 0 || res.verify_phoneflag == "N") {
                    yield this.api.isVerificationPendingModel();
                }
            }
            else {
                this.getCard();
            }
        }));
    }
    getCard() {
        this.storage.get(_api_petcloud_api_service__WEBPACK_IMPORTED_MODULE_2__.PetcloudApiService.STRIPECARD)
            .then((cardData) => {
            if (cardData != null && cardData !== '') {
                this.creditCardFrm.setValue({
                    cardName: cardData.name,
                    cardNumber: 'xxxxxxxxxxxx' + cardData.last4,
                    cardMonth: cardData.exp_month < 9 ? 0 + cardData.exp_month.toString() :
                        cardData.exp_month.toString(),
                    cardYear: cardData.exp_year,
                    cardCcv: ''
                });
            }
        });
    }
    /**
     * set stripe default payment mehtod.
     * @param ev event of field
     */
    setDefaultMethod(ev) {
        if (this.stripeDefaultCheck === true) {
            this.stripeDefaultid = 1;
        }
        else {
            this.stripeDefaultid = '';
        }
    }
    /**
     * Save card in stripe using api
     */
    saveCardInStripe() {
        const card = {
            number: this.creditCardFrm.value.cardNumber,
            name: this.creditCardFrm.value.cardName,
            expMonth: this.creditCardFrm.value.cardMonth,
            expYear: this.creditCardFrm.value.cardYear,
            cvc: this.creditCardFrm.value.cardCcv
        };
        this.api.showLoader();
        this.stripe.createCardToken(card)
            .then((token) => {
            if (token) {
                // send details in stripe
                const cardFrm = {
                    'post_type': 'add_card',
                    'redirect': '',
                    'stripeDefault': '',
                    'stripeToken': token.id
                };
                console.log('cardFrm..............', cardFrm);
                this.creditCardFrm.setValue({
                    cardName: token.card.name,
                    cardNumber: 'xxxxxxxxxxxx' + token.card.last4,
                    cardMonth: token.card.exp_month,
                    cardYear: token.card.exp_year,
                    cardCcv: ''
                });
                this.api.updateCardInStripe(cardFrm)
                    .pipe((0,rxjs_operators__WEBPACK_IMPORTED_MODULE_6__.finalize)(() => {
                    this.api.hideLoader();
                }))
                    .subscribe((res) => {
                    if (res.success) {
                        this.storage.set(_api_petcloud_api_service__WEBPACK_IMPORTED_MODULE_2__.PetcloudApiService.STRIPECARD, token.card);
                        this.api.showToast('card details updated in stripe', 2000, 'bottom');
                        this.navCntl.navigateRoot('/home/tabs/profile-menu');
                    }
                    else {
                        this.api.showToast(res.error, 2000, 'bottom');
                    }
                }, (err) => {
                    this.api.autoLogout(err, cardFrm);
                });
            }
            else {
                this.navCntl.navigateRoot('/home/tabs/profile-menu');
                this.api.showToast('card details not updated in stripe, Please try again!', 3000, 'bottom');
            }
        })
            .catch((error) => {
            this.api.hideLoader();
            this.api.showToast(error, 3000, 'bottom');
        });
    }
    /**
     * convert number to array
     * @param num pass number
     */
    getCardYears() {
        const today = new Date();
        const startYear = today.getFullYear();
        const endYear = (today.getFullYear() + 30);
        const yearArr = [];
        for (let i = startYear; i <= endYear; i++) {
            yearArr.push(i);
        }
        return yearArr;
    }
    connectPaypal() {
        this.api.showToast("We will release paypal payments in next version of App", "3000", "bottom");
    }
    getInfo() {
        this.storage.get(_api_petcloud_api_service__WEBPACK_IMPORTED_MODULE_2__.PetcloudApiService.USER).then((user) => {
            console.log(user.stripe_card);
