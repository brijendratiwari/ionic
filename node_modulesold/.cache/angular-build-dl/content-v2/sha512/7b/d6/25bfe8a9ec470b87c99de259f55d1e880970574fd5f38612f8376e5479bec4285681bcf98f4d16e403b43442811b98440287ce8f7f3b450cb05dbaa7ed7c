let PetAddPage = class PetAddPage {
    constructor(platform, actionSheetCtrl, formBuilder, api, router, cameraAPI, camera, nav, navCtrl, storage) {
        this.platform = platform;
        this.actionSheetCtrl = actionSheetCtrl;
        this.formBuilder = formBuilder;
        this.api = api;
        this.router = router;
        this.cameraAPI = cameraAPI;
        this.camera = camera;
        this.nav = nav;
        this.navCtrl = navCtrl;
        this.storage = storage;
        this.lastImage = null;
        this.todayDate = new Date();
        this.selectedPetImage = '';
        this.petChildrenStaySelectoption = {
            header: 'Fine to stay with children?',
            translucent: true
        };
        this.newPetid = 0; // store new added pet id for update
        this.selectedSegment = '';
        //Segment Click Disable
        this.disablePetInfo = true; // true
        this.disableContactSegment = true; // true
        this.isPetAddedSuccess = false;
        // public disableImageSegment = true;
        this.typePet = null;
        this.currnetDt = new Date().toISOString();
        this.getPreviousURL();
    }
    ngOnInit() {
        this.petForm = this.formBuilder.group({
            petType: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_5__.Validators.required]],
            name: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_5__.Validators.required]],
            gender: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_5__.Validators.required]],
            dob: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_5__.Validators.required]],
            beed: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_5__.Validators.required]],
            pet_fleas: [''],
            desexed: [''],
            children_stay: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_5__.Validators.required]],
            vet_contact: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_5__.Validators.required]],
            vet_amount: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_5__.Validators.required]],
            module: "pets",
            imagename: '',
            imageurl: "", // complete url of uploadurl
        });
        this.selectedSegment = 'image';
    }
    showActionSheet() {
        return (0,tslib__WEBPACK_IMPORTED_MODULE_6__.__awaiter)(this, void 0, void 0, function* () {
            const actionSheet = yield this.actionSheetCtrl.create({
                header: 'Select Profile Photo From',
                buttons: [{
                        text: 'Camera',
                        handler: () => {
                            this.photoOption(this.camera.PictureSourceType.CAMERA);
                        }
                    }, {
                        text: 'Gallery',
                        handler: () => (0,tslib__WEBPACK_IMPORTED_MODULE_6__.__awaiter)(this, void 0, void 0, function* () {
                            const status = yield this.cameraAPI.checkPhotoLibraryPermission();
                            if (status) {
                                this.photoOption(this.camera.PictureSourceType.PHOTOLIBRARY);
                            }
                        })
                    }, {
                        text: 'Cancel',
                        handler: () => {
                            console.log('Cancel clicked');
                        }
                    }]
            });
            yield actionSheet.present();
        });
    }
    photoOption(params) {
        this.cameraAPI.getPicture(params).then((base64) => {
            this.myImageUrl = 'data:image/jpeg;base64,' + base64;
            this.imageUpload();
        }, err => {
        });
    }
    imageUpload() {
        const fileParams = { data: this.myImageUrl, name: "Imagename.jpg" };
        this.api.showLoader();
        this.api.uploadImagefile(fileParams).pipe((0,rxjs_operators__WEBPACK_IMPORTED_MODULE_7__.finalize)(() => {
            this.api.hideLoader();
        }))
            .subscribe((res) => {
            if (res.success) {
                this.resfileName = res.filename;
                this.resimageurl = res.uploadUrl;
                this.api.showToast('File Uploaded.', 2000, 'bottom');
                this.disablePetInfo = false;
                this.disableContactSegment = false;
                this.selectedSegment = "petInfo";
            }
            else {
                this.api.showToast("Error: Image upload failed.", 2000, "bottom");
            }
        }, (err) => {
            this.api.autoLogout(err, fileParams);
        });
    }
    /**
     * Save new pet
     */
    saveNewPet() {
        this.petForm.value.imagename = this.resfileName;
        this.petForm.value.imageurl = this.resimageurl;
        this.api.showLoader();
        this.api.savePet(this.petForm.value).pipe((0,rxjs_operators__WEBPACK_IMPORTED_MODULE_7__.finalize)(() => {
            this.api.hideLoader();
        })).subscribe((res) => {
            console.log(res);
            if (res.success) {
                this.isPetAddedSuccess = true;
                this.newPetid = res.petId;
                this.selectedSegment = "response";
                localStorage.setItem('newPetId', res.petId);
                this.petForm.reset();
            }
            else {
                this.isPetAddedSuccess = false;
            }
        }, (err) => {
            this.isPetAddedSuccess = false;
            this.api.autoLogout(err, this.petForm.value);
        });
    }
    getPreviousURL() {
        this.storage.get('pervURL').then((val) => {
            this.prevURL = val;
        });
    }
    onChangeFineToStay(event) {
        if (this.petForm.value.petType != "" && this.petForm.value.name != "" &&
            this.petForm.value.gender != "" && this.petForm.value.dob != "" &&
            this.petForm.value.beed != "") {
            this.selectedSegment = "contact";
        }
        else {
            this.selectedSegment = "petInfo";
        }
    }
    goToPetListing() {
        this.nav.pop();
    }
};
PetAddPage.ctorParameters = () => [
