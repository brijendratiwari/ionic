/* harmony import */ var _ionic_angular__WEBPACK_IMPORTED_MODULE_6__ = __webpack_require__(/*! @ionic/angular */ 80476);
/* harmony import */ var _angular_router__WEBPACK_IMPORTED_MODULE_5__ = __webpack_require__(/*! @angular/router */ 39895);
/* harmony import */ var rxjs_operators__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(/*! rxjs/operators */ 68939);
/* harmony import */ var _ionic_storage__WEBPACK_IMPORTED_MODULE_4__ = __webpack_require__(/*! @ionic/storage */ 38605);









let RecentJobListingPage = class RecentJobListingPage {
    constructor(storage, router, api, navCtrl) {
        this.storage = storage;
        this.router = router;
        this.api = api;
        this.navCtrl = navCtrl;
        this.recentSitterandJobPostingParams = { latitude: "", longitude: "", distance: "", offset: "" };
        //Response
        this.recentJobs = [];
        this.moreDatarecentJobs = [];
        //Initilize variable count and offset to get more data from Server form loadData Method
        this.count = 0;
        this.offset = 6;
    }
    ngOnInit() {
        this.getInfo();
    }
    getInfo() {
        this.storage.get(_api_petcloud_api_service__WEBPACK_IMPORTED_MODULE_2__.PetcloudApiService.USER)
            .then((res) => {
            this.recentSitterandJobPostingParams.latitude = res.latitude;
            this.recentSitterandJobPostingParams.longitude = res.longitude;
            this.recentSitterandJobPostingParams.distance = "60030";
            this.recentSitterandJobPostingParams.offset = "0";
            this.recentJobPost();
        });
    }
    ionViewDidEnter() {
        this.storage.get(_api_petcloud_api_service__WEBPACK_IMPORTED_MODULE_2__.PetcloudApiService.USER)
            .then((res) => {
            this.recentSitterandJobPostingParams.latitude = res.latitude;
            this.recentSitterandJobPostingParams.longitude = res.longitude;
            this.recentSitterandJobPostingParams.distance = '60030';
            this.recentJobPost();
        });
    }
    /**
    * Get Recent Near By Job Post
    * @param distance is static value passed as 30
    */
    recentJobPost() {
        this.recentSitterandJobPostingParams.offset = '0';
        this.api.recentJobPosted(this.recentSitterandJobPostingParams).pipe((0,rxjs_operators__WEBPACK_IMPORTED_MODULE_3__.finalize)(() => {
        })).subscribe((res) => {
            if (res.success) {
                this.recentJobs = res;
            }
        }, err => {
            if (err.status == 401) {
                this.storage.clear();
                this.api.showToast(this.api.logoutmsg, 2000, 'bottom');
                this.navCtrl.navigateRoot('/get-started');
            }
            else {
                this.api.showToast("Try Again", 2000, "bottom");
            }
        });
    }
    /**
   * Load Data Pagination Method..
   */
    loadData(infiniteScroll) {
        this.count = parseInt(this.count) + 1;
        this.recentSitterandJobPostingParams.offset = this.count * this.offset;
        this.api.recentJobPosted(this.recentSitterandJobPostingParams).pipe((0,rxjs_operators__WEBPACK_IMPORTED_MODULE_3__.finalize)(() => {
        })).subscribe((res) => {
            if (res.success) {
                this.moreDatarecentJobs = res;
                if (this.moreDatarecentJobs.length > 0) {
                    this.recentJobs.push(this.moreDatarecentJobs);
                }
                else {
                    infiniteScroll.target.complete();
                }
                this.count = this.count++;
            }
        }, err => {
            this.api.autoLogout(err, this.recentSitterandJobPostingParams);
            infiniteScroll.target.complete();
        });
    }
};
RecentJobListingPage.ctorParameters = () => [
    { type: _ionic_storage__WEBPACK_IMPORTED_MODULE_4__.Storage },
    { type: _angular_router__WEBPACK_IMPORTED_MODULE_5__.Router },
    { type: _api_petcloud_api_service__WEBPACK_IMPORTED_MODULE_2__.PetcloudApiService },
    { type: _ionic_angular__WEBPACK_IMPORTED_MODULE_6__.NavController }
