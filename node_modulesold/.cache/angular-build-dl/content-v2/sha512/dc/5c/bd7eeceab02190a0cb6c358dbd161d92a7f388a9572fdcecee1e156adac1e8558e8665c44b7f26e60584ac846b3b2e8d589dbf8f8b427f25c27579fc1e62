        }, {
          key: "handleAddressChange",
          value: function handleAddressChange(address) {
            var addressArray = address.formatted_address.split(' ');
            var addressLenth = addressArray.length;
            var addressText = '';

            for (var i = 0; i < addressLenth - 2; i++) {
              addressText += addressArray[i] + '';
            } // set automatice state and pincode value


            this.profileForm.patchValue({
              address: address.vicinity,
              state: this.getState(addressArray[addressLenth - 2]),
              zipcode: isNaN(addressArray[addressLenth - 1]) ? '' : addressArray[addressLenth - 1]
            });
            this.lng = address.geometry.location.lng();
            this.lat = address.geometry.location.lat();
          }
        }, {
          key: "getState",
          value: function getState(stateName) {
            switch (stateName) {
              case 'ACT':
                return '1';
                break;

              case 'NSW':
                return '2';
                break;

              case 'NT':
                return '3';
                break;

              case 'QLD':
                return '4';
                break;

              case 'SA':
                return '5';
                break;

              case 'VIC':
                return '6';
                break;

              case 'WA':
                return '7';
                break;

              case 'TAS':
                return '8';
                break;

              default:
                return '0';
            }
          }
