n\n    constructor(public api: PetcloudApiService,\n        public storage: Storage,public navCtrl: NavController) {\n    }\n\n    ngOnInit() {\n    }\n\n    ionViewDidEnter() {\n        this.getTransactionHistory();\n    }\n\n    /**\n     * Get Transaction history list\n     */\n    public getTransactionHistory() {\n        this.api.showLoader();\n        this.api.transactionHistory()\n            .pipe(finalize(() => {\n                this.api.hideLoader();\n            }))\n            .subscribe((res: any) => {\n                this.transactionHistory = res;\n                this.paymentsHistory = this.transactionHistory.paymentsArray;\n                this.expenseHistory = this.transactionHistory.expensesArray;\n                this.referral = this.transactionHistory.referral\n            }, (err: any) => {\n                // this.api.showToast('no transaction found.', 2000, 'bottom');\n                console.log('error from fetching transaction history', err);\n                this.api.autoLogout(err,\"\")\n            });\n    }\n\n}\n","export default \".transaction-list {\\n  color: white;\\n}\\n.transaction-list .transaction-item {\\n  display: flex;\\n  flex-direction: row;\\n  justify-content: flex-start;\\n  padding: 5px 15px 0;\\n  border-bottom: 1px solid #EBEBEB;\\n}\\n.transaction-list .transaction-item .name {\\n  padding-right: 15px;\\n}\\n.transaction-list .transaction-item .name p {\\n  font-weight: 500;\\n  font-size: 14px;\\n  color: #4A4A4A;\\n  letter-spacing: 0;\\n  text-align: center;\\n  line-height: 18px;\\n  margin: 5px;\\n}\\n.transaction-list .transaction-item .info .transid {\\n  font-weight: 600;\\n  font-size: 14px;\\n  color: #4A4A4A;\\n  letter-spacing: 0;\\n  line-height: 22px;\\n  margin: 5px;\\n}\\n.transaction-list .transaction-item .info .date {\\n  font-size: 12px;\\n  color: #888888;\\n  letter-spacing: 0;\\n  line-height: 14px;\\n  margin: 5px;\\n}\\n.transaction-list .transaction-item .info .complete {\\n  font-weigh