let RemindersListPage = class RemindersListPage {
    constructor(api) {
        this.api = api;
        this.remindersList = [];
        this.isAPILoaded = false;
        this.pageNo = 1;
        this.isPaignation = false;
    }
    ngOnInit() {
    }
    ionViewWillEnter() {
        this.isAPILoaded = false;
        this.remindersList = [];
        this.reminders();
        this.getRemindersList("");
    }
    notificationReminder(notificationCode) {
        let statusName = "";
        switch (notificationCode) {
            case "0": {
                statusName = "Send before a hour";
                break;
            }
            case "1": {
                statusName = "Send at a time";
                break;
            }
            case "2": {
                statusName = "Send one day before";
                break;
            }
        }
        return statusName;
    }
    getReminderType(reminderType) {
        return _api_petcloud_api_service__WEBPACK_IMPORTED_MODULE_2__.PetcloudApiService.remindersType(reminderType);
    }
    reminderFrequency(reminderFreq) {
        return _api_petcloud_api_service__WEBPACK_IMPORTED_MODULE_2__.PetcloudApiService.reminderFrequency(reminderFreq);
    }
    reminders() {
        fetch('assets/JSON/reminders.json')
            .then(res => res.json())
            .then((data) => {
            this.reminderTypes = data;
        });
    }
    getRemindersList(infiniteScroll) {
        if (this.pageNo == 1) {
            this.api.showLoader();
        }
        this.api.getReminderListAlert(this.pageNo).pipe((0,rxjs_operators__WEBPACK_IMPORTED_MODULE_3__.finalize)(() => {
            if (this.pageNo == 1) {
                this.api.hideLoader();
            }
        })).subscribe((res) => {
            this.isAPILoaded = true;
            res.pages > this.pageNo ? this.isPaignation = true : this.isPaignation = false;
            if (res.success) {
                if (res.data.length) {
                    if (infiniteScroll) {
                        infiniteScroll.target.complete();
                    }
                    if (res.data.length) {
                        res.data.forEach(((data) => (0,tslib__WEBPACK_IMPORTED_MODULE_4__.__awaiter)(this, void 0, void 0, function* () {
                            this.remindersList.push(data);
                        })));
                    }
                }
                else {
                    if (infiniteScroll) {
                        infiniteScroll.target.complete();
                    }
                }
            }
            else {
                if (infiniteScroll) {
                    infiniteScroll.target.complete();
                }
            }
        }, err => {
            this.api.autoLogout(err, "");
        });
    }
    loadData(event) {
        this.pageNo = this.pageNo + 1;
        this.getRemindersList(event);
    }
