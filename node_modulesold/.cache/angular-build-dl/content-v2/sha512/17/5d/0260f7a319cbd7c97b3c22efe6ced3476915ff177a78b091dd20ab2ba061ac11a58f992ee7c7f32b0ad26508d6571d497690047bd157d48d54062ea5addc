avigateByUrl('/home/tabs/profile-menu')\n        });\n        return await modal.present();\n      } else {\n\n        const formData = {\n          WalletTransaction: {\n            payout_source: this.withdraw.value.payout_source,\n            amount: this.withdraw.value.amount\n          }\n        }\n        this.api.showLoader();\n        this.api.walletWithdraw(formData)\n          .pipe(finalize(() => {\n            this.api.hideLoader();\n          })).subscribe((res: any) => {\n            if (res.success) {\n              this.api.showToast(res.message, '2000', 'bottom');\n              this.router.navigateByUrl('/home/tabs/profile-menu')\n            } else {\n              this.api.showToast(res.message, '2000', 'bottom');\n            }\n          }, (err: any) => {\n            this.api.autoLogout(err,formData)\n            this.api.hideLoader();\n          });\n      }\n    }\n\n  }\n}\n","export default \".card-list .item {\\n  padding: 16px;\\n  background-color: #f1f1f1;\\n  border-radius: 5px;\\n  display: flex;\\n}\\n.card-list .item .card-data {\\n  margin-left: auto;\\n  text-align: right;\\n}\\n.card-list .item .card-data .card-no {\\n  margin: 0;\\n  font-size: 14px;\\n  margin-bottom: 5px;\\n  white-space: nowrap;\\n}\\n.card-list .item .card-data .remove-card {\\n  margin: 0;\\n  font-size: 12px;\\n  font-weight: 400;\\n  opacity: 0.5;\\n}\\n.card-list .item .details .description {\\n  margin: 0;\\n  font-size: 14px;\\n  margin-bottom: 5px;\\n  color: #fe4164;\\n}\\n.card-list .item .details .make-default-card {\\n  margin: 0;\\n  font-size: 12px;\\n  font-weight: 400;\\n  opacity: 0.5;\\n}\\n.update-bank-container {\\n  margin-top: 10px;\\n}\\n/*# sourceMappingURL=data:application/json;base64, */\";","export default \".withdraw-button {\\n  position: fixed;\\n  bottom: 0;\\n  width: 100%;\\n  text-align: center;\\n  text-decoration: none;\\n  padding: 14px;\\n  background-color: #fe4164;\\n  color: #fff;\\n}\\n/*# sourceMappingURL=data:app