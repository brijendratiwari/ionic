                        skipLocationChange: true,
                    });
                }
                else {
                    this.api.showToast("Availability not updated.", 2000, "bottom");
                }
            }, (err) => {
                this.api.hideLoader();
                this.api.autoLogout(err, this.selectedBlockDay);
            });
        }
