    ionViewDidEnter() {
        this.meetingData = [];
        let today = new Date();
        let currentMonth = today.getMonth() + 1;
        let month = today.getMonth() + 1 < 10 ? "0" + currentMonth : currentMonth;
        let year = today.getFullYear();
