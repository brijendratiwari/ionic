            event.newMonth.months < 10
                ? "0" + event.newMonth.months
                : event.newMonth.months;
        let selectedMonth = this.currentActiveYear + "-" + this.currentActiveMonth;
        this.selectedMth = this.currentActiveYear + "-" + this.currentActiveMonth;
