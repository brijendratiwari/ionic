    }
    facebookLogin() {
        // show loader
        this.api.showLoader();
        // the permissions your facebook app needs from the user
        const permissionsArr = ["public_profile", "email"];
        this.fb
            .login(permissionsArr)
            .then((res) => (0,tslib__WEBPACK_IMPORTED_MODULE_11__.__awaiter)(this, void 0, void 0, function* () {
            const deviceData = yield this.api.getFirebaseToken();
            const deviceId = {
                androidId: deviceData.androidId,
                iPhoneId: deviceData.iPhoneId,
                token: yield res.authResponse.accessToken
            };
            this.api.loginWithFacebook(deviceId).subscribe((r) => {
                this.api.hideLoader();
                if (r.success === true) {
                    if (r.user && r.token) {
                        this.storage.set(_api_petcloud_api_service__WEBPACK_IMPORTED_MODULE_2__.PetcloudApiService.USERTOKEN, r.token);
                        localStorage.setItem("token", r.token);
                        this.events.publish("token", r.token),
                            deviceId.androidId != "" ? this.appFlyerAnalytics.registerToken(deviceId.androidId) :
                                this.appFlyerAnalytics.registerToken(deviceId.iPhoneId);
                        this.storage.set(_api_petcloud_api_service__WEBPACK_IMPORTED_MODULE_2__.PetcloudApiService.USER, r.user);
                        this.api.checkIsNewUser().then((dynamicUrl) => {
                            this.navCntl.navigateRoot(dynamicUrl);
                        });
                    }
                    else {
                        this.api.showToast("User is not logged in by Facebook! Try again", 2000, "bottom");
                    }
                }
                else {
                    this.api.showToast("User is not logged in by Facebook! Try again", 2000, "bottom");
                }
            }, (err) => {
                this.api.hideLoader();
                this.api.showToast("User is not logged in! Try Again", 2000, "bottom");
            });
        }))
            .catch((e) => {
            this.api.hideLoader();
