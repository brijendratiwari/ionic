    facebookVerify() {
        this.fb
            .login(["email", "public_profile"])
            .then((response) => {
            this.fb
                .api("me?fields=id,name,email,first_name,picture.width(720).height(720).as(picture_large)", [])
                .then((profile) => {
                this.userData = {
                    id: profile["id"],
                    email: profile["email"],
                    first_name: profile["first_name"],
                    picture: profile["picture_large"]["data"]["url"],
                    username: profile["name"],
                };
                let fbParams = { access_token: this.userData.id };
                this.api.showLoader();
                this.api
                    .facebookVerify(fbParams)
                    .pipe((0,rxjs_operators__WEBPACK_IMPORTED_MODULE_11__.finalize)(() => {
                    this.api.hideLoader();
                }))
                    .subscribe((res) => {
                    if (res.success) {
                        this.checkVerification();
                        this.api.showToast("Profile Verified", 2000, "bottom");
                        if (this.backgroundCheck &&
                            this.isPhoneVerified &&
                            this.isEmailVerified) {
                            this.navCtrl.navigateRoot("home/tabs/profile-menu");
                        }
                    }
                    else {
                        this.api.showToast("That Facebook account is already linked to a PetCloud account.", 2000, "bottom");
                    }
                }, (err) => {
                    this.api.autoLogout(err, "");
                });
            });
        });
