            }
        }, (err) => {
            this.api.showToast(err, 3000, 'bottom');
            this.api.autoLogout(err, bankFrm);
        });
    }
    backButtonNavigate() {
        if (this.backButton) {
            this.router.navigateByUrl('/home/tabs/jobs-tab');
