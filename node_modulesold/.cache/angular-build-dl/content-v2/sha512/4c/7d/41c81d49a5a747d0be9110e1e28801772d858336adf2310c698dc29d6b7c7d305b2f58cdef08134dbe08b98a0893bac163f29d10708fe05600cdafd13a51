        this.CameraAPI.getPicture(params).then((base64String) => {
            this.imageUpload('data:image/jpeg;base64,' + base64String, pageSide);
        }, err => {
        });
    }
    imageUpload(base64String, pageSide) {
        const fileParams = { image: base64String, file_name: "Imagename.jpg", pageside: pageSide };
        this.api.showLoader();
        this.api.uploadDocumentForBank(fileParams).pipe((0,rxjs_operators__WEBPACK_IMPORTED_MODULE_7__.finalize)(() => {
            this.api.hideLoader();
        }))
            .subscribe((res) => {
            if (res.success) {
