        this.isStripeAccountAdded = false;
        this.documentContainer = false;
        this.backButton = this.route.snapshot.paramMap.get("backBtn");
        this.backButtonEvent();
    }
    ngOnInit() {
        this.stripeFrm = this.formBuilder.group({
            accountNumber: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_5__.Validators.required]],
            accountName: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_5__.Validators.required]],
            bsb: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_5__.Validators.required]]
        });
        this.paypalFrm = this.formBuilder.group({
            paypal_email: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_5__.Validators.required, _angular_forms__WEBPACK_IMPORTED_MODULE_5__.Validators.email]]
        });
        this.selectedSegment = 'stripe';
