nk-details_module_ts-es2015.js","sourcesContent":["import { NgModule } from '@angular/core';\nimport { CommonModule } from '@angular/common';\nimport { FormsModule } from '@angular/forms';\nimport { Routes, RouterModule } from '@angular/router';\n\nimport { IonicModule } from '@ionic/angular';\n\nimport { BankDetailsPage } from './bank-details.page';\n\nconst routes: Routes = [\n  {\n    path: '',\n    component: BankDetailsPage\n  }\n];\n\n@NgModule({\n  imports: [\n    CommonModule,\n    FormsModule,\n    IonicModule,\n    RouterModule.forChild(routes)\n  ],\n  declarations: [BankDetailsPage]\n})\nexport class BankDetailsPageModule {}\n","import { Component, OnInit } from '@angular/core';\nimport { PetcloudApiService } from '../api/petcloud-api.service';\nimport { finalize } from 'rxjs/operators';\nimport { Router } from '@angular/router';\n\n@Component({\n  selector: 'app-bank-details',\n  templateUrl: './bank-details.page.html',\n  styleUrls: ['./bank-details.page.scss'],\n})\nexport class BankDetailsPage implements OnInit {\n\n  public bankDetails: any = \"\"\n  constructor(public api: PetcloudApiService,\n    public router: Router) { }\n\n  ngOnInit() {\n    this.getBankDetails();\n  }\n\n  needSupport() {\n    this.api.openExteralLinks(this.api.FRESHDESK_WEB)\n  }\n\n  getBankDetails(){\n    this.api.showLoader();\n    this.api.bankDetail().pipe(\n        finalize(() => {\n          this.api.hideLoader();\n        })\n      ).subscribe(\n        (res: any) => {\n          {\n            if(res.status){\n              this.bankDetails = res;\n            }else{\n              this.api.showToast(res.message,\"3000\",\"bottom\")\n            }\n            \n          }\n        },\n        (err: any) => {\n          this.api.autoLogout(err,\"\");\n        }\n      );\n  }\n\n  paymentSend(){\n    this.router.navigateByUrl(\"/addmoney\")\n  }\n}\n","export default \"ion-button.payment-send {\\n  margin: 0;\\n}\\n\\n.button-large {\\n  font-size: 14px;\\n  height: 53px;\\n}\\n\\n.bank-transfer-title {\\n  padding: 16px;\\n  text-align: center;\\n}\\n\\n.bank-transfer-title h3 {\\n  font-size: 18px;\\n  margin-bottom: 5px;\\n}\\n\\n.bank-transfer-title p {\\n  margin: 0;\\n  opacity: 0.5;\\n}\\n\\n.bank-transfer-title a {\\n  margin-bottom: 10px;\\n  margin-top: 10px;\\n  display: inline-block;\\n}\\n\\n.bank-details h4 {\\n  font-size: 18px;\\n  font-weight: 500;\\n  text-align: center;\\n  border-bottom: 1px solid #e0e0e0;\\n  padding-bottom: 20px;\\n  color: #000;\\n}\\n\\n.bank-details h6 {\\n  font-size: 14px;\\n  color: #000;\\n}\\n/*# sourceMappingURL=data:application/json;base64, */\";","export default \"<ion-header>\\n  <ion-toolbar>\\n    <ion-buttons slot=\\\"start\\\">\\n      <ion-back-button defaultHref=\\\"/home/tabs/profile-menu\\\"></ion-back-button>\\n    </ion-buttons>\\n    <ion-title>Bank Details</ion-title>\\n\\n    <ion-buttons slot=\\\"end\\\">\\n      <ion-button (click)=\\\"needSupport()\\\" defaultHref=\\\"/home/tabs/profile-menu\\\">\\n        <img width=\\\"25px\\\" src=\\\"./assets/icon/solutions.png\\\" alt=\\\"\\\" />\\n      </ion-button>\\n    </ion-buttons>\\n  </ion-toolbar>\\n</ion-header>\\n\\n<ion-content>\\n  <div class=\\\"bank-transfer-title\\\">\\n    <h3>Our bank details for this transfer</h3>\\n    <p>Go to your online banking and transfer to our account using the details below</p>\\n    <a routerLink=\\\"/addmoney\\\">Or, pay another way</a>\\n  </div>\\n  <ion-card class=\\\"bank-details\\\">\\n    <ion-card-content>\\n      <ion-row>\\n        <ion-col size=\\\"12\\\">\\n          <h4>Bank transfer details</h4>\\n        </ion-col>\\n      </ion-row>\\n      <ion-row>\\n\\n        <ion-col size=\\\"6\\\">\\n          <h5>Payee name</h5>\\n          <h6>Pet Cloud</h6>\\n        </ion-col>\\n        <ion-col size=\\\"6\\\">\\n          <h5>Payment description</h5>\\n          <h6>{{bankDetails?.payment_description}}</h6>\\n        </ion-col>\\n      </ion-row>\\n      <ion-row>\\