let AddBankDetailPayoutComponent = class AddBankDetailPayoutComponent {
    constructor(formBuilder, modelCntl, navParam, api) {
        this.formBuilder = formBuilder;
        this.modelCntl = modelCntl;
        this.navParam = navParam;
        this.api = api;
        this.showBankForm = false;
        this.isBankDetailShown = false;
        this.addBankSubscription = new rxjs__WEBPACK_IMPORTED_MODULE_3__.Subscription();
        this.amt = this.navParam.get('amount');
    }
    ngOnInit() {
        this.addBank = this.formBuilder.group({
            accountName: ["", [_angular_forms__WEBPACK_IMPORTED_MODULE_4__.Validators.required]],
            bsb: ["", [_angular_forms__WEBPACK_IMPORTED_MODULE_4__.Validators.required]],
            accountNumber: ["", [_angular_forms__WEBPACK_IMPORTED_MODULE_4__.Validators.required]],
            amount: [this.amt, [_angular_forms__WEBPACK_IMPORTED_MODULE_4__.Validators.required]]
        });
        this.getLastBankPayout();
    }
    needSupport() {
        this.api.openExteralLinks(this.api.FRESHDESK_WEB);
    }
    closeModal() {
        this.addBankSubscription.unsubscribe();
        this.modelCntl.dismiss();
    }
    addBankDetails() {
        const addBankForm = {
            amount: this.addBank.value.amount,
            BankForm: {
                accountName: this.addBank.value.accountName,
                bsb: this.addBank.value.bsb,
                accountNumber: this.addBank.value.accountNumber
            }
        };
        this.api.showLoader();
        this.addBankSubscription.add(this.api.sendBankPayout(addBankForm).pipe((0,rxjs_operators__WEBPACK_IMPORTED_MODULE_5__.finalize)(() => {
            this.api.hideLoader();
        })).subscribe((res) => {
            {
                if (res.status) {
                    this.closeModal();
                    this.api.showToast(res.message, 2000, 'bottom');
                }
                else {
                    this.api.showToast(res.error, 2000, 'bottom');
                }
            }
        }, (err) => {
            this.closeModal();
            this.api.autoLogout(err, addBankForm);
        }));
    }
    toggleBankAccount() {
        this.showBankForm == true ? this.showBankForm = false : this.showBankForm = true;
    }
    transferNow() {
        const transferData = {
            amount: this.amt,
        };
        this.api.showLoader();
        this.api.directBankPayout(transferData).pipe((0,rxjs_operators__WEBPACK_IMPORTED_MODULE_5__.finalize)(() => {
            this.api.hideLoader();
        })).subscribe((resp) => {
            if (resp.status) {
                this.api.showToast(resp.message, "3000", "bottom");
                this.closeModal();
            }
            else {
                this.api.showToast(resp.message, "3000", "bottom");
            }
        }, err => {
            this.api.autoLogout(err, transferData);
        });
    }
    getLastBankPayout() {
        this.api.showLoader();
        this.api.getRecentBank().pipe((0,rxjs_operators__WEBPACK_IMPORTED_MODULE_5__.finalize)(() => {
            this.api.hideLoader();
        })).subscribe((resp) => (0,tslib__WEBPACK_IMPORTED_MODULE_6__.__awaiter)(this, void 0, void 0, function* () {
            if (yield resp.status) {
                if (resp.account) {
                    this.accountNo = yield resp.account.accountNumber;
                    this.bankName = yield resp.bank_name;
                    this.isBankDetailShown = true;
                }
                else {
                    this.showBankForm = true;
                    this.isBankDetailShown = false;
                }
            }
            else {
                this.showBankForm = true;
            }
        }), err => {
            this.showBankForm = true;
            this.api.autoLogout(err, "");
        });
    }
};
