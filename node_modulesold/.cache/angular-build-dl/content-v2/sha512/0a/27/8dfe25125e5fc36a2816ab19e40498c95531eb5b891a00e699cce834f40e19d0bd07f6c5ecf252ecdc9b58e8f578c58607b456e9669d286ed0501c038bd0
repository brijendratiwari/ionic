        if (pageName == "post-job") {
            this.post_job = true;
            this.view_jobs = false;
            this.jobs_posted = false;
        }
        else if (pageName == "view-jobs") {
            this.post_job = false;
            this.view_jobs = true;
            this.jobs_posted = false;
        }
