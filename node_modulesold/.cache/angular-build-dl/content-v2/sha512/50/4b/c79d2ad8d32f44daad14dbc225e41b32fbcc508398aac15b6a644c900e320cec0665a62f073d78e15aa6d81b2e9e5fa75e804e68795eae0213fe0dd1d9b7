        }, {
          key: "getDates",
          value: function getDates(startDate, stopDate) {
            var dateArray = [];
            var fromDate;
            var endDt;
            fromDate = moment__WEBPACK_IMPORTED_MODULE_3__(startDate).format("YYYY-MM-DD");
            endDt = moment__WEBPACK_IMPORTED_MODULE_3__(stopDate).format("YYYY-MM-DD");
            var dateArray = [];
