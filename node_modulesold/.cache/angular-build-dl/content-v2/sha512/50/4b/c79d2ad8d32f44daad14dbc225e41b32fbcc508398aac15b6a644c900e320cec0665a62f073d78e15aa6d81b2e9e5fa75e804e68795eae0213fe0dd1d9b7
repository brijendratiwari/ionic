                if (this.jobPostingForm.value.recurring_type != this.CUSTOM || this.jobPostingForm.value.recurring_type != this.ONCE_A_WEEK) {
                  if (this.jobPostingForm.value.recurring_type != "") {
                    this.isFormValid = true;
                  } else {
